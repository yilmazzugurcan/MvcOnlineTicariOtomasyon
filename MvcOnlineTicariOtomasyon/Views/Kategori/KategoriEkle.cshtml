@model MvcOnlineTicariOtomasyon.Models.Class.Kategori
@{
    ViewBag.Title = "KategoriEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<br />
<h2 style="margin-left:20px">Yeni Kategori Sayfası</h2>
<br />
<form class="form-group" method="post" style="margin-left:20px">
    @Html.LabelFor(x=>x.KategoriAd)
    @Html.TextBoxFor(x=>x.KategoriAd,new {@class = "form-control"})
    <br />
    <button class="btn btn-info">Kaydet</button>


</form>

